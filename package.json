{
  "name": "shorten-repo-url",
  "version": "1.5.3",
  "description": "Shorten GitHub URLs like GitHub shortens Issues and Commit URLs",
  "keywords": [
    "commit",
    "file",
    "github",
    "gitlab",
    "issue",
    "link",
    "links",
    "ref",
    "reference",
    "shorter",
    "url"
  ],
  "repository": "fregante/shorten-repo-url",
  "license": "MIT",
  "author": "Federico Brigante <opensource@bfred.it> (bfred.it)",
  "files": [
    "index.js",
    "browser-url.js",
    "index.d.ts"
  ],
  "browser": {
    "url": "./browser-url"
  },
  "scripts": {
    "test": "xo && tsd && ava"
  },
  "xo": {
    "envs": [
      "browser"
    ],
    "extensions": [
      "ts"
    ],
    "overrides": [
      {
        "files": "**/*.ts",
        "extends": "xo-typescript"
      }
    ],
    "rules": {
      "no-redeclare": 0
    }
  },
  "dependencies": {
    "github-reserved-names": "^1.0.3"
  },
  "devDependencies": {
    "@sindresorhus/tsconfig": "^0.3.0",
    "@typescript-eslint/eslint-plugin": "^1.4.2",
    "@typescript-eslint/parser": "^1.12.0",
    "ava": "*",
    "eslint-config-xo-typescript": "^0.9.0",
    "tsd": "^0.7.2",
    "xo": "*"
  },
  "engines": {
    "node": ">=7.0.0"
  }
}
